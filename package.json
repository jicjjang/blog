{
  "name": "blog",
  "version": "1.0.0",
  "description": "This blog has been used since 2018 using Nuxtjs with vuejs for github pages blog",
  "author": "jicjjang <jicjjang12@gmail.com>",
  "private": false,
  "scripts": {
    "dev": "cross-env NODE_ENV=development nuxt",
    "build": "nuxt build",
    "prestart": "npm run build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "prepublish": "npm run generate",
    "publish": "push-dir --dir=dist --branch=gh-pages --cleanup",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "precommit": "npm run lint",
    "storybook": "start-storybook -p 9001 -c .storybook"
  },
  "dependencies": {
    "@nuxtjs/markdownit": "^1.2.3",
    "@nuxtjs/sitemap": "0.0.3",
    "axios": "^0.18.1",
    "marked": "^0.3.19",
    "nuxt": "^1.4.5"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^3.4.11",
    "@storybook/addon-centered": "^3.4.11",
    "@storybook/vue": "^3.4.11",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.6",
    "cross-env": "^5.2.0",
    "eslint": "^4.19.1",
    "eslint-friendly-formatter": "^3.0.0",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-vue": "^4.7.1",
    "node-sass": "^4.13.0",
    "push-dir": "^0.4.1",
    "sass-loader": "^6.0.6"
  }
}
